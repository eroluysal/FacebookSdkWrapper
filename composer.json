{
    "name": "eroluysal/facebook-sdk-wrapper",
    "type": "Laravel Bundle",
    "version": "1.0.0-dev",
    "description": "Facebook -v4- Sdk Wrapper",
    "authors": [
        {
            "name": "Erol Uysal",
            "email": "eutkuuysal@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QweB\\Facebook\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "facebook/php-sdk-v4": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "eutkuuysal@gmail.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0@dev",
        "mockery/mockery": "^1.0@dev"
    }
}
